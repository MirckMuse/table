{
  "name": "@scode/table-state-rs",
  "version": "0.0.1",
  "private": "true",
  "description": "A Table State Library for @scode/table",
  "main": "index.js",
  "scripts": {
    "build": "pnpm build-wasm && pnpm clean-wasm-build",
    "build:release": "pnpm build-wasm:release && pnpm clean-wasm-build",
    "build-wasm": "wasm-pack build --dev --target web -d ./dist",
    "build-wasm:release": "wasm-pack build --release --target web -d ./dist",
    "clean-wasm-build": "rimraf ./dist/.gitignore ./dist/package.json ./dist/README.md"
  },
  "files": [
    "./dist/scode_table_state_rs_bg.wasm.d.ts",
    "./dist/scode_table_state_rs_bg.wasm",
    "./dist/scode_table_state_rs.js",
    "./dist/scode_table_state_rs.d.ts"
  ],
  "module": "./dist/scode_table_state_rs.js",
  "types": "./dist/scode_table_state_rs.d.ts",
  "keywords": [
    "rust",
    "table",
    "state"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@scode/table-typing": "workspace:*"
  },
  "devDependencies": {
    "rimraf": "^5.0.5"
  }
}